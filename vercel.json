{
  "version": 2,
  "name": "8bitwear-wizard-v2",
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "mockups/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*\\.(js|css|html|jpg|jpeg|png))",
      "dest": "/$1"
    },
    {
      "src": "/mockups/(.*)",
      "dest": "/mockups/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}

